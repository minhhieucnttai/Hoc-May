{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: Current File",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "${file}",
                "--server.port=8501"
            ],
            "python": "${workspaceFolder}/.venv311/Scripts/python.exe",
            "jinja": true,
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            }
        },
        {
            "name": "Python: Current File (.venv311)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "${workspaceFolder}/.venv311/Scripts/python.exe",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
